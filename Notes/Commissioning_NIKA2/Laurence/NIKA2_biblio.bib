	journal = {\aap},
	author = {Adam, R. and Comis, B. and Macías-Pérez, J. F. and Adane, A. and Ade, P. and André, P. and Beelen, A. and Belier, B. and Benoît, A. and Bideaud, A. and Billot, N. and Boudou, N. and Bourrion, O. and Calvo, M. and Catalano, A. and Coiffard, G. and D'Addabbo, A. and Désert, F.-X. and Doyle, S. and Goupy, J. and Kramer, C. and Leclercq, S. and Martino, J. and Mauskopf, P. and Mayet, F. and Monfardini, A. and Pajot, F. and Pascale, E. and Perotto, L. and Pointecouteau, E. and Ponthieu, N. and Revéret, V. and Rodriguez, L. and Savini, G. and Schuster, K. and Sievers, A. and Tucker, C. and Zylka, R.},
	month = sep,
